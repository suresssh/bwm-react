{
  "name": "bwm-react",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "mongoose": "^5.4.13",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.3",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "titleize": "^1.0.1"
  },
  "scripts": {
    "watch-css": "node-sass-chokidar ./src -o ./src",
    "build-css": "npm run build-css && node-sass-chokidar ./src -o ./src --watch --recursive",
    "start": "npm-run-all -p watch-css start-js start-dev",
    "start-js": "react-scripts start",
    "start-dev": "node ./server/index.js",
    "build": "npm-run-all -p build-css build-js",
    "build-js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
